/* @import url(http://fonts.googleapis.com/earlyaccess/notosanskr.css); */

/* NotoSansKR 웹폰트 설치 */
@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 100;
    src: url(../fonts/NotoSansKr/NotoSansKR-Thin.woff2) format('font-woff2'),
    url(../fonts/NotoSansKr/NotoSansKR-Thin.woff) format('woff'),
    url(../fonts/NotoSansKr/NotoSansKR-Thin.otf) format('opentype');
}

@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 300;
    src: url(../fonts/NotoSansKr/NotoSansKR-Light.woff2) format('font-woff2'),
    url(../fonts/NotoSansKr/NotoSansKR-Light.woff) format('woff'),
    url(../fonts/NotoSansKr/NotoSansKR-Light.otf) format('opentype');
}

@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 400;
    src: url(../fonts/NotoSansKr/NotoSansKR-Regular.woff2) format('font-woff2'),
    url(../fonts/NotoSansKr/NotoSansKR-Regular.woff) format('woff'),
    url(../fonts/NotoSansKr/NotoSansKR-Regular.otf) format('opentype');
}

@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 500;
    src: url(../fonts/NotoSansKr/NotoSansKR-Medium.woff2) format('font-woff2'),
    url(../fonts/NotoSansKr/NotoSansKR-Medium.woff) format('woff'),
    url(../fonts/NotoSansKr/NotoSansKR-Medium.otf) format('opentype');
}

@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 700;
    src: url(../fonts/NotoSansKr/NotoSansKR-Bold.woff2) format('font-woff2'),
    url(../fonts/NotoSansKr/NotoSansKR-Bold.woff) format('woff'),
    url(../fonts/NotoSansKr/NotoSansKR-Bold.otf) format('opentype');
}

@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 900;
    src: url(../fonts/NotoSansKr/NotoSansKR-Black.woff2) format('font-woff2'),
    url(../fonts/NotoSansKr/NotoSansKR-Black.woff) format('woff'),
    url(../fonts/NotoSansKr/NotoSansKR-Black.otf) format('opentype');
}

/* NotoSansKR 웹폰트 설치 end*/

html,
body {
    width: 100%;
    height: 100%;
    min-width: 1280px;
}

body {
    color: #333;
    font-family: NotoSansKR-Regular, 'Noto Sans KR';
    font-size: 14px;
}

button,
button:active,
button:focus,
button:hover,
.btn,
.btn:active,
.btn:focus,
.btn:hover,
label::before,
input,
input:focus,
textarea,
textarea:focus,
select,
select:focus {
    outline: none !important;
    box-shadow: none !important;
}

ul,
li {
    margin: 0px;
    padding: 0px;
    list-style: none;
}

p,
dl,
dt,
dd,
h1,
h2,
h3,
h4,
h5,
h6 {
    margin: 0px;
}

a {
    display: block;
    color: #333;
    text-decoration-line: none !important;
    cursor: pointer;
}

.table caption {
    position: absolute;
    top: -9999px;
    left: -9999px;
    width: 1px;
    height: 1px;
}

/*form*/
.form-control {
    padding: 14px;
    height: 40px;
    font-size: 14px;
    background-color: #c2ccd5;
    border: 1px solid #ddd;
    border-radius: 0px;
}

.input_lg {
    padding: 22px 22px 22px 18px;
    height: 60px;
    font-size: 16px;
    color: #212121;
    background-color: #c2ccd5;
    border: 0px;
    border-radius: 0px;
}

.form-select {
    padding: 9px;
    height: 40px;
    font-size: 14px;
    background-color: #c2ccd5;
    border: 1px solid #ddd;
    border-radius: 0px;
}

/*form end*/

/*button*/
.btn {
    padding: 8px 30px;
    font-size: 16px;
    font-weight: normal;
    border: none;
    border-radius: 4px;
}

.btn_blue {
    color: #fff;
    background-color: #0073d1;
}

.btn_blue:hover,
.btn_blue:focus {
    color: #fff !important;
    background-color: #00447c !important;
}

.btn_blue:active {
    color: #fff !important;
    background-color: #002849 !important;
}

.btn_blue:disabled {
    color: #fff !important;
    background-color: #ccdfef !important;
}

.btn_grey {
    color: #333;
    background-color: #ddd;
}

.btn_grey:hover,
.btn_grey:focus {
    color: #333 !important;
    background-color: #b3b3b3 !important;
}

.btn_grey:active {
    color: #333 !important;
    background-color: #7c7c7c !important;
}

.btn_grey:disabled {
    color: #777 !important;
    background-color: #f0f0f0 !important;
}

.btn_black {
    color: #fff;
    background-color: #4a4a4a;
}

.btn_black:hover,
.btn_black:focus {
    color: #fff !important;
    background-color: #363636 !important;
}

.btn_black:active {
    color: #fff !important;
    background-color: #202020 !important;
}

.btn_black:disabled {
    color: #fff !important;
    background-color: #9f9f9f !important;
}

.btn_red {
    color: #fff;
    background-color: #f50000;
}

.btn_red:hover,
.btn_red:focus {
    color: #fff !important;
    background-color: #cf0303 !important;
}

.btn_red:active {
    color: #fff !important;
    background-color: #990404 !important;
}

.btn_red:disabled {
    color: #fff !important;
    background-color: #eb8080 !important;
}

.btn_lg {
    height: 60px;
    border-radius: 0;
}
/*button end*/

/*scrollbar*/
/* 아래의 모든 코드는 영역::코드로 사용 */
.scrollbar_custom {
    overflow: auto;
}

.scrollbar_custom::-webkit-scrollbar {
    width: 5px;
    height: 5px;
    /* 스크롤바의 너비 */
}

.scrollbar_custom::-webkit-scrollbar-thumb {
    width: 30%;
    /* 스크롤바의 길이 */
    background: #aaaaaa;
    /* 스크롤바의 색상 */

    border-radius: 9px;
}

.scrollbar_custom::-webkit-scrollbar-track {
    background: #e1e1e1;
    /*스크롤바 뒷 배경 색상*/
}

/*scrollbar end*/

/*text*/
.text_grey {
    color: #777;
}

.desc_center {
    text-align: center;
}

.text_red {
    color: #f50000 !important;
}

.text_green {
    color: #0fb100;
}

.text_Success {
    color: #0ced17;
}

.text_blue {
    color: #0073d1;
}

.text_yellow {
    color: #f7d23f;
}

/*text end*/

/*table*/
.table {
    width: 100%;
    margin-top: 10px;
    margin-bottom: 0;
}

.table thead th {
    padding: 5px;
    color: #fff;
    background-color: #2e4861;
    border-bottom: 1px solid #fff;
    vertical-align: middle;
}

.table tbody td {
    padding: 5px;
    color: #fff;
    border-bottom: 1px solid #ddd;
    vertical-align: middle;
}

.table tbody tr:nth-child(odd) td {
    background-color: #214365;
}

.table.table_bordered thead th,
.table.table_bordered tbody td {
    border: 1px solid #daecff;
}

.table.table_bordered thead th:first-child,
.table.table_bordered tbody td:first-child {
    border-left: 0;
}

.table.table_bordered thead th:last-child,
.table.table_bordered tbody td:last-child {
    border-right: 0;
}

.table tbody td .table_a {
    margin-top: 4px;
    color: #fff;
    text-decoration: underline !important;
}

.table tbody td .table_a:first-child {
    margin-top: 0;
}

.table tbody td .table_a:hover {
    color: #0d6efd;
}

.table_2 {
    width: 100%;
    margin-top: 10px;
    margin-bottom: 0;
}

.table_2 thead th {
    padding: 5px;
    color: #fff;
    border: 1px solid #fff;
    vertical-align: middle;
}

.table_2 tbody td {
    padding: 5px;
    color: #fff;
    border: 1px solid #ddd;
    vertical-align: middle;
}

/*table end*/

/*table_input*/
.table_input_title_btn {
    display: flex;
    align-items: end;
}

.table_input_title h4 {
    font-size: 20px;
    color: #ffffff;
    font-weight: 500;
}

.table_input_contents {
    margin-top: 10px;
    border-top: 2px solid #ccc;
}

.table_input {
    display: table;
    width: 100%;
    table-layout: fixed;
}

.table_input_th {
    display: table-cell;
    padding: 10px;
    width: 200px;
    color: #fff;
    background-color: #2e4861;
    border-bottom: 1px solid #ccc;
    vertical-align: middle;
}

.th_input {
    padding: 4px;
}

.table_input_td {
    display: table-cell;
    padding: 4px;
    width: calc(100% - 200px);
    color: #fff;
    background-color: #05223f;
    border-bottom: 1px solid #ccc;
}

.table_input_td textarea {
    height: unset;
}

.td_btn .form-control {
    display: inline-block;
    width: calc(100% - 50px);
}

.td_btn .form-select {
    display: inline-block;
    width: calc(100% - 50px);
}

.td_btn .btn {
    margin-left: 4px;
    padding: 6px 12px 13px 13px;
    height: 40px;
    background-color: #eff8ff !important;
    vertical-align: top;
}

.td_btn .btn img {
    width: 15px;
}

/*table_input end*/

/*table_search*/
.table_search_wrap .table_input_contents {
    display: flex;
    border-top: 0;
}

.table_search_wrap .table_input.ti_1_3 {
    width: calc(100% / 3);
}

.table_search_wrap .table_input.ti_2_3 {
    width: calc((100% / 3) * 2);
}

.table_search_wrap .table_input_th {
    border-bottom: 0;
}

.table_search_wrap .table_input_td {
    padding-top: 0;
    padding-bottom: 0;
    border-bottom: 0;
}

.table_search_wrap .table_input_datepicker .datepicker {
    display: inline-block;
    /*width: calc((100% - 265px)/2);*/
}

.table_search_wrap .table_input_datepicker .dp_btn {
    display: inline-block;
    vertical-align: top;
}

.table_search_wrap .table_input_datepicker .dp_btn .btn {
    padding: 9px 20px 10px;
    font-size: 14px;
}

/*table_search end*/

/*pagination*/
.pagination_wrap {
    display: flex;
    justify-content: center;
    padding: 30px 0;
    text-align: center;
}

.pagination_wrap .pagination li a {
    margin-left: 4px;
    padding: 4px;
    width: 32px;
    height: 32px;
    color: #777;
    background-color: #c2ccd5;
    border: 1px solid #ddd;
}

.pagination_wrap .pagination li:nth-child(2) a {
    margin-left: 0px;
}

.pagination_wrap .pagination li.on a,
.pagination_wrap .pagination li a:hover {
    color: #fff;
    font-weight: bold;
    background-color: #4a4a4a;
    border: 1px solid #4a4a4a;
}

.pagination_wrap .pagination li a.prev,
.pagination_wrap .pagination li a.next {
    margin: 0 12px;
    padding: 3px;
    background-color: transparent;
    border: 0;
}

.pagination_wrap .pagination li a.prev:hover,
.pagination_wrap .pagination li a.next:hover {
    background-color: #979ea5;
}

.pagination_wrap .pagination li.disabled a.prev,
.pagination_wrap .pagination li.disabled a.next {
    background-color: #979ea5;
    cursor: not-allowed;
}

.pagination_wrap .pagination li a.prev img,
.pagination_wrap .pagination li a.next img {
    width: 15px;
}

/*pagination end*/

/*login*/
.login_wrap {
    display: flex;
    align-items: center;
    height: 100vh;
    background-color: #031e39;
}

.login_box {
    margin: 0 auto;
    padding: 20px;
    width: 400px;
    background-color: #2e4861;
    border-radius: 12px;
}

.login_header h1 {
    padding: 10px 0 40px 10px;
    font-size: 40px;
    color: #fff;
    font-weight: 700;
}

/*login end*/

/*wrapper*/
.wrapper {
    width: 100%;
    height: 100%;
}

/*wrapper end*/

/*header*/
.header_wrap {
    width: 100%;
    background-color: #031e39;
    border-bottom: 1px solid #fff;
}

.header {
    display: flex;
    align-items: center;
    padding: 0 10px;
    width: 100%;
}

.header_wrap h1 {
    display: flex;
}

.gnb_wrap {
    padding: 0 20px;
}

.gnb {
    display: flex;
    align-items: center;
}

.gnb_item {
    position: relative;
}

.gnb_item>a {
    display: flex;
    align-items: center;
    padding: 0 10px;
    height: 50px;
    color: #aaa;
    text-align: center;
}

.gnb_item.on>a,
.gnb_item.active>a {
    color: #fff;
    font-weight: 500;
}

.gnb_sub_item>a{
    color: #ffffff !important;
}

.gnb_main::before {
    display: inline-block;
    content: '';
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_main_g.png);
}

.gnb_item.on .gnb_main::before,
.gnb_item.active .gnb_main::before {
    background-image: url(../img/main/icon_main_w.png);
}

.gnb_testresult::before {
    display: inline-block;
    content: '';
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_testresult_g.png);
}

.gnb_item.on .gnb_testresult::before,
.gnb_item.active .gnb_testresult::before {
    background-image: url(../img/main/icon_testresult_w.png);
}

.gnb_eventLog::before {
    display: inline-block;
    content: '';
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_testresult_g.png);
}

.gnb_item.on .gnb_eventLog::before,
.gnb_item.active .gnb_eventLog::before {
    background-image: url(../img/main/icon_testresult_w.png);
}

.gnb_setting::before {
    display: inline-block;
    content: '';
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_setting_g.png);
}

.gnb_item.on .gnb_setting::before,
.gnb_item.active .gnb_setting::before {
    background-image: url(../img/main/icon_setting_w.png);
}

.gnb_setting::after {
    display: inline-block;
    content: '';
    margin-left: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_down_arrow_g.png);
}

.gnb_item.on .gnb_setting::after,
.gnb_item.active .gnb_setting::after {
    background-image: url(../img/main/icon_down_arrow_w.png);
}

.gnb_sub {
    display: none;
    position: absolute;
    top: 100%;
    left: 0;
    padding: 10px 0;
    width: 140px;
    background-color: #031e39;
    z-index: 1;
}

.gnb_item.on .gnb_sub {
    display: block;
}

.gnb_sub_item>a {
    padding: 5px;
    color: #aaa;
    text-align: center;
}

.gnb_sub_item>a:hover {
    color: #fff;
    font-weight: 500;
}

.top_wrap {
    display: flex;
    margin-left: auto;
    padding: 0 10px;
}

.top_wrap.login_lan_wrap {
    display: flex;
    flex-direction: row;
    margin-bottom: 10px;
    padding: 0;
    justify-content: space-between;
}

.top_wrap .btn {
    display: flex;
    align-items: center;
    position: relative;
    padding: 0 10px;
    height: 50px;
    font-size: 12px;
    color: #aaa;
}

.top_wrap .btn.on,
.top_wrap .btn:active {
    color: #fff;
}

.btn_global::before {
    display: inline-block;
    content: '';
    margin-right: 4px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_global_g.png);
}

.top_wrap .btn.on.btn_global::before {
    background-image: url(../img/main/icon_global_w.png);
}

.btn_global::after {
    display: inline-block;
    content: '';
    margin-left: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_down_arrow_g.png);
}

.top_wrap .btn.on.btn_global::after {
    background-image: url(../img/main/icon_down_arrow_w.png);
}

.btn_logout::before {
    display: inline-block;
    content: '';
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/main/icon_logout_g.png);
}

.top_wrap .btn.on.btn_logout::before {
    background-image: url(../img/main/icon_logout_w.png);
}

.top_wrap .global_sub {
    display: none;
    position: absolute;
    top: 100%;
    left: 0;
    padding: 10px 0;
    width: 100%;
    background-color: #031e39;
    z-index: 1;
}

.top_wrap .btn.on .global_sub {
    display: block;
}

.global_sub a {
    padding: 2px;
    color: #aaa;
}

.global_sub a:hover {
    color: #fff;
}

/*header end*/

/*container*/
.masterContainer {
    display: flex;
    padding: 0;
    width: 100%;
    max-width: unset;
    height: calc(100vh - 51px);
}

/*container end*/

/*side bar*/
.side_bar {
    width: 270px;
    height: 100%;
    background-color: #031e39;
}

.side_bar_box {
    height: 100%;
}

.side_bar_title h2 {
    position: relative;
    padding: 30px 10px;
    font-size: 24px;
    color: #fff;
    font-weight: 500;
    text-align: center;
    border-bottom: 1px solid #eee;
}

.side_bar_title_comp {
    color: #000000 !important;
}

.side_bar_con {
    height: 100%;
}

.side_bar_title+.side_bar_con {
    height: calc(100% - 89.8px);
}

.scroll_box {
    max-height: 100%;
}

.scroll_box_2 {
    max-height: calc(100% - 60px);
}

.simulator_item a,
.simulator_item_gnb a {
    display: flex;
    align-items: center;
    padding: 0px 10px;
    width: 100%;
    height: 40px;
    color: #fff !important;
    line-height: 40px;
}
.icon_battery {
    justify-content: space-between !important;
}

.simulator_item a:hover,
.simulator_item_gnb a:hover {
    background-color: #053360;
}

.simulator_item.active a {
    color: #053360 !important;
    font-weight: 700;
    background-color: #c2ccd5;
}

.simulator_item_gnb a span {
    display: flex;
    align-items: center;
}

.icon_dashboard::before {
    display: inline-block;
    content: '';
    margin-left: 16px;
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/side_bar/icon_dashboard_w.png);
}

.simulator_item.active a.icon_dashboard::before {
    background-image: url(../img/side_bar/icon_dashboard_b.png);
}

.icon_folder_arrow::before {
    display: inline-block;
    content: '';
    margin-right: 8px;
    width: 8px;
    height: 8px;
    background-image: url(../img/side_bar/icon_folder_arrow_w.png);
}

.simulator_item_gnb.on>a.icon_folder_arrow::before {
    transform: rotate(45deg);
}

.icon_simultor_management::before {
    display: inline-block;
    content: '';
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/side_bar/icon_folder_w.png);
}

.simulator_sub_list {
    display: none;
}

.simulator_item_gnb.on .simulator_sub_list {
    display: block;
}

.simulator_item_sub a::before {
    display: inline-block;
    content: '';
    margin-left: 32px;
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/side_bar/icon_electric_charge_w.png);
}

.simulator_list>.simulator_item_sub a::before {
    margin-left: 0;
}

.simulator_item_sub.active a::before {
    background-image: url(../img/side_bar/icon_electric_charge_b.png);
}

.simulator_item_sub a span {
    display: inline-block;
    width: calc(100% - 50px);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.icon_battery::before {
    display: inline-block;
    content: '';
    margin-right: 2px;
    width: 16px;
    height: 16px;
    background-image: url(../img/side_bar/icon_battery_w.png);
}

.simulator_item.active a.icon_battery::before {
    background-image: url(../img/side_bar/icon_battery_b.png);
}
/*side bar end*/

/*contents_wrap*/
.contents_wrap {
    position: relative;
    width: 100%;
    height: 100%;
    background-color: #05223f;
}

.contents_wrap_2 {
    position: relative;
    width: 100%;
    height: 100%;
    background-color: #05223f;
}

.contents_title_wrap {
    display: flex;
    align-items: end;
    padding: 26px 0 24px;
    border-bottom: 1px solid #ddd;
}

.contents_title_wrap h3 {
    font-size: 30px;
    font-weight: 500;
}

.sub_location img {
    width: 14px;
}

.contents_info {
    margin-top: 20px;
    padding: 10px;
    background-color: #f0f0f0;
}

.contents_info img {
    width: 20px;
}

.contents_box {
    padding: 20px 0;
}

.contents {
    padding: 30px;
    background-color: #05223f;
}

